package org.netxilia.api.storage;

public class DataSourceConfigurationId {
	private final int id;

	public DataSourceConfigurationId(int id) {
		this.id = id;
	}

	public DataSourceConfigurationId(String id) {
		this.id = Integer.valueOf(id);
	}

	public int getId() {
		return id;
	}

	@Override
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime * result + id;
		return result;
	}

	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		DataSourceConfigurationId other = (DataSourceConfigurationId) obj;
		if (id != other.id)
			return false;
		return true;
	}

	@Override
	public String toString() {
		return Integer.toString(id);
	}

}
