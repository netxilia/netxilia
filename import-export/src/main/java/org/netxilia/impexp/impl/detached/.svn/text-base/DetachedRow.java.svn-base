package org.netxilia.impexp.impl.detached;

import java.util.ArrayList;
import java.util.List;

import org.netxilia.api.display.Styles;

public class DetachedRow {
	public static final int DEFAULT_HEIGHT = 0;

	private Integer height;
	private Styles styles;
	private List<DetachedCell> cells = new ArrayList<DetachedCell>();

	public List<DetachedCell> getCells() {
		return cells;
	}

	public void setCells(List<DetachedCell> cells) {
		this.cells = cells;
	}

	public Integer getHeight() {
		return height;
	}

	public Styles getStyles() {
		return styles;
	}

	public void setHeight(Integer height) {
		this.height = height;
	}

	public void setStyles(Styles styles) {
		this.styles = styles;
	}

}
